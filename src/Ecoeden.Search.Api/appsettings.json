{
  "Logging": {
    "IncludeScopes": false,
    "LogOutputTemplate": "[{Timestamp:dd-MM-yyyy HH:mm:ss, fff}] {Level} {Properties} [CorrelationId:{CorrelationId}] [Machine: {MachineName}] {CallerType}:{MemberName} {Message}{NewLine}{Exception}",
    "Console": {
      "Enabled": true,
      "LogLevel": "Debug"
    },
    "Elastic": {
      "Enabled": true,
      "LogLevel": "Debug"
    }
  },
  "AppConfigurations": {
    "ApplicationIdentifier": "Ecoeden.Search.Api",
    "ApplicationEnvironment": "Development",
    "HealthCheckTimeOutInSeconds": 2
  },
  "ApiDescription": "Ecoeden search api description",
  "ApiOriginHost": "localhost:5003",
  "ElasticSearch": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*"
}
